@model DivisionListViewModel

@{
    ViewData["Title"] = SetCurrentOrganizationActionFilter.CurrentOrganization;
}

<h1>@SetCurrentOrganizationActionFilter.CurrentOrganization</h1>

<table class="table">
    <tbody>
        @foreach (var item in Model.DivisionsList)
        {
            <tr>
                <td>
                    <a asp-action="Index" asp-controller="Standings" asp-route-organization="@SetCurrentOrganizationActionFilter.CurrentOrganization" asp-route-id="@item.ID">@Html.DisplayFor(modelItem => item.League) @Html.DisplayFor(modelItem => item.NameOrNumber)</a>
                </td>
            </tr>
        }
    </tbody>
</table>