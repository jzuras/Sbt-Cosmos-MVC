@model DivisionInfo

@{
    ViewData["Title"] = "Admin - Division Details";
}

<h1>Division Details</h1>

<h4>Organization: @SetCurrentOrganizationActionFilter.CurrentOrganization</h4>
<hr />
<div>
    <dl class="row">
        <dt class="col-sm-2">
            @Html.DisplayNameFor(model => model.ID)
        </dt>
        <dd class="col-sm-10">
            @Html.DisplayFor(model => model.ID)
        </dd>
        <dt class="col-sm-2">
            @Html.DisplayNameFor(model => model.League)
        </dt>
        <dd class="col-sm-10">
            @Html.DisplayFor(model => model.League)
        </dd>
        <dt class="col-sm-2">
            Name or #
        </dt>
        <dd class="col-sm-10">
            @Html.DisplayFor(model => model.NameOrNumber)
        </dd>
        <dt class="col-sm-2">
            @Html.DisplayNameFor(model => model.Locked)
        </dt>
        <dd class="col-sm-10">
            @Html.DisplayFor(model => model.Locked)
        </dd>
        <dt class="col-sm-2">
            @Html.DisplayNameFor(model => model.Updated)
        </dt>
        <dd class="col-sm-10">
            @Html.DisplayFor(model => model.Updated)
        </dd>
    </dl>
</div>
<div>
    <a asp-action="Edit" asp-route-id="@Model.ID" asp-route-organization="@SetCurrentOrganizationActionFilter.CurrentOrganization">Edit</a> |
    <a asp-action="Index" asp-route-organization="@SetCurrentOrganizationActionFilter.CurrentOrganization">Back to List</a>
</div>
